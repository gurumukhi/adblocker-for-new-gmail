{
  "manifest_version": 2,
  "name": "AdFree Gmail",
  "description": "Removes all annoying Ads and Banners from Gmail on both old and new Gmail UI [AbBlocker for Gmail]",
  "version": "0.1.2",
  "icons": {
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.gmail.com/*",
        "*://mail.google.com/*"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ]
}